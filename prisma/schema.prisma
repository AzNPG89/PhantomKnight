generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("mongodbUrl")
}

model automods {
  id      String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  v       Int      @default(0) @map("__v")
  enabled Boolean
  guildId String   @unique
  words   String[]
}

model emojis {
  id         String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  v          Int    @default(0) @map("__v")
  customName String
  data       String
}

model playlists {
  id       String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  v        Int      @default(0) @map("__v")
  playList String[]
  userId   String   @unique
}

model users {
  id            String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  userId        String @unique
  walletBalance BigInt
  bankBalance   BigInt
  inventory     Json[]
  lastWorked    BigInt
}

model warnings {
  id       String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  v        Int    @default(0) @map("__v")
  guildId  String
  warnings Json[]
}

model welcomers {
  id              String  @id @default(dbgenerated()) @map("_id") @db.ObjectId
  v               Int     @default(0) @map("__v")
  channelId       String
  guildId         String  @unique
  welcomerMessage String
  enabled         Boolean
}

model leveling {
  id        String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  exp       Int
  level     Int
  nextLevel Int
  levelUpXp Int
  userId    String @unique
}
